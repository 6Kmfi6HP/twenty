{
  "$schema": "https://json.schemastore.org/babelrc.json",
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript",
    ["@babel/preset-react", {"runtime": "automatic"}]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@ui": "./src",
          "@assets": "./assets"
        }
      }
    ],
    "inline-react-svg",
    [
      "inline-import",
      {
        "extensions": [".png", ".jpg", ".jpeg"]
      }
    ]
  ],
  "overrides": [
    {
      "include": [
        "**/OverflowingTextWithTooltip.tsx",
        "**/Tag.tsx",
        "**/Avatar.tsx",
        "**/Chip.tsx",
        "**/ContactLink.tsx",
        "**/RoundedLink.tsx"
      ],
      "presets": [
        [
          "@wyw-in-js",
          {
            "evaluate": false,
            "displayName": false,
            "preprocessor": "none"
          }
        ]
      ]
    }
  ]
}
