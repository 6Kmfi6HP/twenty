{
  "$schema": "https://json.schemastore.org/babelrc.json",
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript",
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@ui": "./src",
          "@assets": "./assets"
        }
      }
    ],
    "inline-react-svg",
    [
      "inline-import",
      {
        "extensions": [
          ".png",
          ".jpg",
          ".jpeg"
        ]
      }
    ]
  ]
}
